<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#333" android:endColor="#333" android:angle="90" android:centerColor="#ddd"/>
            <stroke android:width="1dp" android:color="#ff333333"/>
        </shape>
    </item>
    <item android:left="4dp" android:top="1dp" android:right="4dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#aaa" android:endColor="#aaa" android:angle="90" android:centerColor="#fff"/>
        </shape>
    </item>
</layer-list>
