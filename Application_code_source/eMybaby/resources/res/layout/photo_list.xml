<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <TableLayout android:id="@+id/tablePhotoList" android:layout_width="wrap_content" android:layout_height="wrap_content" android:stretchColumns="2">
        <TableRow android:id="@+id/tableRowPhotoListHeader" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <TextView android:id="@+id/txtPhotoListHeaderName" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <TextView android:id="@+id/txtPhotoListHeaderCreateDate" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </TableRow>
    </TableLayout>
</LinearLayout>
