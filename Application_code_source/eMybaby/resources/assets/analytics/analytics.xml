    <categories>
        
        <category code="MIBEBE">
            <events>
                <event code="BEBE_GUARDADO"/>
            </events>
        </category>
        <category code="CALENDARIO">
            <events>
                <event code="EVENTO_GUARDADO"/>
            </events>
        </category>
        <category code="VIGILABEBE">
            <events>
                <event code="PLAY"/>
                <event code="PAUSE"/>
            </events>
        </category>
        <category code="FICHA_MEDICA">
            <events>
                <event code="CITA_MEDICA_GUARDADA"/>
                <event code="VACUNA_GUARDADA"/>
                <event code="MEDIDA_GUARDADA"/>
            </events>
        </category>
        <category code="TERMOMETRO">
            <events>
                <event code="MEDIDA_MANUAL"/>
                <event code="MEDIDA_AUTOMATICA" description="La etiqueta será el tipo de termómetro: thermoadvanced plus (sonido), thermoadvanced pharma (Bluetooth)"/>
            </events>
        </category>
        <category code="MY_SCALE">
            <events>
                <event code="MEDIDA_MANUAL"/>
                <event code="MEDIDA_AUTOMATICA"/>
            </events>
        </category>
        <category code="MY_BUDDY">
            <events>
                <event code="RECORD"/>
                <event code="PLAY" description="La etiqueta será el nombre de la canción"/>
                <event code="ADD_FAVORITE" description="La etiqueta será el nombre de la canción"/>
                <event code="REMOVE_FAVORITE" description="La etiqueta será el nombre de la canción"/>
                <event code="PLAYER_SECTION" description="La etiqueta será el nombre de la categoría (Parent Sounds, lullabies,...)"/>
            </events>
        </category>
        <category code="MI_EMBARAZO">
            <events>
                <event code="SWEETBEAT_RECORD" />
                <event code="SWEETBEAT_PLAY" />
                <event code="SWEETBEAT_SHARE" />
                <event code="BEBE_NUEVA_ECO" />
                <event code="BEBE_NUEVOS_DATOS" />
                <event code="MAMA_NUEVO_PESO" />
                <event code="MAMA_NUEVA_TENSION" />
                <event code="MAMA_MOV_FETAL" />
                <event code="MAMA_NUEVA_MEDIDA" />
                <event code="MAMA_NUEVA_TRIPITA" />
                <event code="CITA_MEDICA_NUEVA" />
                <event code="CONTRAC_INICIO" />
                <event code="CONTRAC_FIN" />
                <event code="DIARIO_NUEVO" />
                <event code="AJUSTES_NUEVO" />
                <event code="AJUSTES_BORRAR" />
            </events>
        </category>
    </categories>
